<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.study.member.repository.MemberRepository">

	<select id="isPossibleLogin" parameterType="kr.co.study.member.dto.MemberDTO" resultType="boolean">
		SELECT *
		FROM MEMBER
		WHERE USER_NAME = #{ userName }
		AND USER_PASSWORD = #{ userPassword }	
	</select>

</mapper>